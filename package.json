{
  "name": "rwsdk-web-extension-starter",
  "version": "1.0.0",
  "private": true,
  "description": "Complete boilerplate for RedwoodSDK web application with companion Chrome extension",
  "scripts": {
    "dev:web": "cd standard && pnpm dev",
    "dev:extension": "cd extension && pnpm dev",
    "build:web": "cd standard && pnpm build",
    "build:extension": "cd extension && pnpm build",
    "deploy:web": "cd standard && pnpm release",
    "package:extension": "cd extension && pnpm zip",
    "format": "prettier --write standard/src extension/src"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.14.4+sha512.c8180b3fbe4e4bca02c94234717896b5529740a6cbadf19fa78254270403ea2f27d4e1d46a08a0f56c89b63dc8ebfd3ee53326da720273794e6200fcf0d184ab",
  "devDependencies": {
    "knip": "^5.50.5",
    "prettier": "^3.5.3"
  }
}
